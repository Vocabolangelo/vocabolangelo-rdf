    skos:example "Il risultato è inaspettato, questa deve essere una regola arbitroia."@it ;
    dct:creator vocang:omiccioliRiccardo .

vocang:serraiolo
    a skos:Concept ;
    skos:prefLabel "Serraiolo"@it ;
